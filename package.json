{
  "name": "odoc",
  "version": "1.5.0",
  "description": "The OCaml/Reason Documentation Generator",
  "repository": "https://github.com/ocaml/odoc",
  "authors": [
    "Thomas Refis <trefis@janestreet.com>",
    "David Sheets <sheets@alum.mit.edu>",
    "Leo White <leo@lpw25.net>"
  ],
  "esy": {
    "build": "dune build -p odoc",
    "release": {
      "releasedBinaries": [
        "odoc"
      ],
      "deleteFromBinaryRelease": [
        "alcotest",
        "bisect_ppx",
        "js-build-tools",
        "lambdasoup",
        "markup",
        "oasis",
        "ocaml-*",
        "ocamlfind",
        "sexplib0"
      ]
    },
    "buildsInSource": "_build"
  },
  "dependencies": {
    "@opam/alcotest": "1.2.2",
    "@opam/astring": "0.8.3",
    "@opam/bisect_ppx": "2.6.0",
    "@opam/cmdliner": "1.0.4",
    "@opam/cppo": "1.6.5",
    "@opam/dune": "2.7.1",
    "@opam/fpath": "0.7.2",
    "@opam/lambdasoup": "0.7.1",
    "@opam/markup": "~1.0.0",
    "@opam/mdx": "1.8.1",
    "@opam/ocamlfind": "1.8.1",
    "@opam/result": "1.5",
    "@opam/sexplib0": "*",
    "@opam/tyxml": "4.3.0",
    "@opam/ocamlfind-secondary": "1.8.1",
    "ocaml": "~4.2.0",
    "@esy-ocaml/reason": "3.7.0"
  },
  "devDependencies": {
    "@opam/ocamlformat": "0.17.0",
    "@opam/merlin": "^3.0.3",
    "ocaml": "~4.9.0",
    "@opam/ocaml-lsp-server": "*"
  },
  "license": "ISC",
  "private": false
}
